{
    "name": "feedback2me-ui",
    "version": "0.0.1",
    "description": "Feedback UI application",
    "main": "src/index.jsx",
    "author": "Burak Buruk",
    "license": "MIT",
    "scripts": {
        "start": "rimraf build & yarn webpack-dev",
        "webpack-dev": "webpack-dev-server --config config/webpack.config.js --open --mode development",
        "build": "rimraf build & yarn webpack-prod",
        "webpack-prod": "webpack --config config/webpack.config.js --mode production",
        "build-image": "docker build . -t feedback2me",
        "run-image": "docker run -p 8000:80 feedback2me",
        "lint": "eslint src/",
        "test": "jest -c jest.config.js",
        "test-watch": "jest -c jest.config.js --watch",
        "test-coverage": "jest -c jest.config.js --coverage --colors"
    },
    "dependencies": {
        "@ant-design/icons": "^4.1.0",
        "antd": "^4.1.2",
        "axios": "^0.19.2",
        "babel-polyfill": "^6.26.0",
        "immutable": "^4.0.0-rc.12",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3",
        "styled-components": "^5.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.9.4",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^25.4.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^3.5.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.4",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-react": "^7.19.0",
        "html-loader": "^1.0.0",
        "html-webpack-plugin": "^4.0.3",
        "husky": "^4.2.5",
        "jest": "^25.4.0",
        "react-test-renderer": "^16.13.1",
        "redux-logger": "^3.0.6",
        "rimraf": "^3.0.2",
        "style-loader": "^1.1.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint",
            "pre-push": "yarn lint"
        }
    }
}
